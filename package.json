    "webpack": "^3.5.5",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-manifest-plugin": "^1.3.1"
  },
  "watch": {
    "./fec/fec/static/js/**/*.js": [
      "build-js"
    ],
    "./fec/fec/static/js/templates/**/*.hbs": [
      "build-js"
    ],
    "./fec/fec/static/scss/**/*.scss": [
      "build-sass"
    ]
  },
  "browserslist": "last 2 versions, > 1%, IE 10",
  "engines": {
    "node": "10.16.0",
    "npm": "6.9.2"
  },
  "license": "CC0-1.0"
}
