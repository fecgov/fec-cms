{
  "name": "fec-cms",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fecgov/fec-cms.git"
  },
  "scripts": {
    "build": "cd fec && webpack && gulp build-sass && gulp build-widgets-sass && gulp purgecss",
    "build-icons": "cd fec && gulp minify-icons && gulp consolidate-icons",
    "build-sass": "cd fec && gulp build-sass && gulp build-widgets-sass && gulp purgecss",
    "build-widgets-sass": "cd fec && gulp build-widgets-sass",
    "build-homepage-css": "cd fec && gulp purgecss",
    "build-js": "cd fec && webpack",
    "build-production": "cd fec && webpack -p && gulp build-sass && gulp build-widgets-sass && gulp purgecss",
    "format": "eslint --fix ./fec/fec/static/js/**/*.js",
    "lint": "eslint --quiet ./fec/fec/static/js/**/*.js",
    "format-lint": "npm run format && npm run lint",
    "test": "karma start",
    "test-single": "npm run lint && karma start --single-run --browsers ChromeHeadless",
    "watch-js": "npm-watch build-js",
    "watch-css": "npm-watch build-sass",
    "watch": "concurrently \"npm run watch-js\" \"npm run watch-css\""
  },
  "dependencies": {
    "a11y-dialog": "4.0.0",
    "accessible-mega-menu": "git+https://github.com/noahmanger/Accessible-Mega-Menu.git",
    "ace-builds": "1.2.2",
    "aria-accordion": "1.0.0",
    "chroma-js": "1.0.1",
    "colorbrewer": "0.0.2",
    "component-sticky": "1.0.0",
    "core-js": "^3.27.2",
    "corejs-typeahead": "^1.2.1",
    "css.escape": "^1.5.1",
    "d3": "3.5.5",
    "datatables.net": "^1.11.3",
    "datatables.net-responsive": "^2.2.7",
    "dompurify": "^2.2.7",
    "draftail": "^1.4.1",
    "es6-weak-map": "2.0.1",
    "eventemitter2": "0.4.14",
    "fullcalendar": "3.3.1",
    "glossary-panel": "1.0.0",
    "graceful-fs": "^4.2.10",
    "jquery": "^3.5.1",
    "jquery.inputmask": "^3.3.4",
    "moment": "2.29.4",
    "numeral": "^1.5.3",
    "perfect-scrollbar": "0.6.2",
    "prop-types": "^15.7.2",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-input-mask": "^2.0.3",
    "sprintf-js": "^1.1.2",
    "topojson-client": "^3.1.0",
    "whatwg-fetch": "^3.5.0"
  },
  "devDependencies": {
    "abortcontroller-polyfill": "^1.3.0",
    "array-from-polyfill": "^1.0.1",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^24.1.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-latest": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babelify": "^7.3.0",
    "browserify": "^16.5.0",
    "browserify-istanbul": "^3.0.1",
    "cache-loader": "^1.2.5",
    "chai": "^3.4.1",
    "clean-webpack-plugin": "^0.1.16",
    "concurrently": "^7.6.0",
    "element-closest": "^3.0.1",
    "element-remove": "^1.0.4",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-react": "^7.26.1",
    "gulp": "^4.0.2",
    "gulp-clean": "^0.4.0",
    "gulp-consolidate": "^0.2.0",
    "gulp-css-urlencode-inline-svgs": "0.0.7",
    "gulp-csso": "^4.0.1",
    "gulp-if": "^2.0.2",
    "gulp-purgecss": "^4.0.3",
    "gulp-rename": "^1.2.2",
    "gulp-rev": "^9.0.0",
    "gulp-sass": "^5.1.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-svgmin": "^2.1.0",
    "handlebars": "^4.7.6",
    "handlebars-template-loader": "^1.0.0",
    "hbsfy": "2.8.1",
    "istanbul-instrumenter-loader": "^3.0.1",
    "karma": "^4.1.0",
    "karma-browserify": "^5.0.0",
    "karma-chai": "^0.1.0",
    "karma-chai-sinon": "^0.1.5",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-mocha": "^0.2.2",
    "karma-webpack": "^3.0.5",
    "leaflet": "^1.9.4",
    "leaflet-providers": "^2.0.0",
    "mocha": "^5.2.0",
    "mockdate": "^2.0.2",
    "npm-watch": "^0.11.0",
    "promise-polyfill": "^8.1.3",
    "puppeteer": "^18.2.1",
    "sass": "^1.60.0",
    "scrollmonitor": "^1.2.3",
    "sinon": "^1.17.2",
    "sinon-chai": "^2.8.0",
    "stringify": "^5.2.0",
    "underscore": "^1.13.1",
    "urijs": "^1.19.1",
    "vue": "^2.6.11",
    "webpack": "^3.5.5",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-manifest-plugin": "^1.3.1"
  },
  "watch": {
    "build-js": {
      "extensions": "js,hbs",
      "patterns": "./fec/fec/static/js/**"
    },
    "build-sass": {
      "extensions": "scss",
      "patterns": "./fec/fec/static/scss/**"
    }
  },
  "browserslist": "last 2 versions, > 1%, IE 10",
  "engines": {
    "node": "18.17.1",
    "npm": "9.6.7"
  },
  "license": "CC0-1.0"
}
