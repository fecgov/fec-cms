{
  "name": "fec-cms",
  "version": "2.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fecgov/fec-cms.git"
  },
  "scripts": {
    "build": "cd fec && webpack --mode development && gulp build-sass && gulp build-widgets-sass && gulp purgecss && gulp build-webp-files",
    "build-icons": "cd fec && gulp minify-icons && gulp consolidate-icons",
    "build-webp": "cd fec && gulp build-webp-files",
    "build-sass": "cd fec && gulp build-sass && gulp build-widgets-sass && gulp purgecss",
    "build-widgets-sass": "cd fec && gulp build-widgets-sass",
    "build-homepage-css": "cd fec && gulp purgecss",
    "build-js": "cd fec && webpack --mode development",
    "build-production": "cd fec && webpack --mode production && gulp build-sass && gulp build-widgets-sass && gulp purgecss && gulp build-webp-files",
    "format": "eslint --fix ./fec/fec/static/js/**/*.js",
    "lint": "eslint --quiet ./fec/fec/static/js/**/*.js",
    "format-lint": "npm run format && npm run lint",
    "test": "karma start",
    "test-single": "npm run lint && karma start --single-run --browsers ChromeHeadless",
    "watch-js": "npm-watch build-js",
    "watch-css": "npm-watch build-sass",
    "watch": "concurrently \"npm run watch-js\" \"npm run watch-css\""
  },
  "dependencies": {
    "accessible-mega-menu": "git+https://github.com/noahmanger/Accessible-Mega-Menu.git",
    "ace-builds": "^1.2.2",
    "d3": "^3.5.17",
    "draftail": "^1.4.1",
    "eventemitter2": "^6.4.9",
    "jquery": "^3.7.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-input-mask": "^2.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/eslint-parser": "^7.24.7",
    "@babel/plugin-syntax-import-assertions": "^7.24.7",
    "@babel/plugin-syntax-import-attributes": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-react": "^7.24.7",
    "@fullcalendar/core": "^5.11.5",
    "@fullcalendar/daygrid": "^5.11.5",
    "@fullcalendar/list": "^5.11.5",
    "a11y-dialog": "^8.0.4",
    "aria-accordion": "^1.1.0",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "babel-polyfill": "^6.26.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babelify": "^10.0.0",
    "browserify": "^16.5.0",
    "browserify-istanbul": "^3.0.1",
    "chai": "^4.4.1",
    "chai-jquery": "^2.1.0",
    "chroma-js": "^2.4.2",
    "clean-webpack-plugin": "^4.0.0",
    "colorbrewer": "^0.0.2",
    "component-sticky": "^1.1.0",
    "concurrently": "^8.2.2",
    "corejs-typeahead": "^1.3.4",
