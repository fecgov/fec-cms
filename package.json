    "gulp-sass": "^4.0.2",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-svgmin": "^2.1.0",
    "gulp-uglify": "3.0.1",
    "handlebars": "^4.1.2",
    "handlebars-template-loader": "^1.0.0",
