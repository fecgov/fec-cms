    "sass": "^1.77.6",
    "scrollmonitor": "^1.2.11",
    "sinon": "^17.0.1",
    "sinon-chai": "^3.7.0",
    "sprintf-js": "^1.1.3",
    "stream-browserify": "^3.0.0",
    "topojson-client": "^3.1.0",
    "underscore": "^1.13.6",
    "urijs": "^1.19.11",
    "vue": "^2.7.16",
    "webpack": "^5.92.1",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "5.1.4",
    "webpack-manifest-plugin": "^5.0.0"
  },
  "watch": {
    "build-js": {
      "extensions": "js,hbs",
      "patterns": "./fec/fec/static/js/**"
    },
    "build-sass": {
      "extensions": "scss",
      "patterns": "./fec/fec/static/scss/**"
    }
  },
  "browserslist": "last 2 versions, > 1%, IE 10",
  "engines": {
    "node": "22.1.0",
    "npm": "10.8.2"
  },
  "license": "CC0-1.0"
}
