{
  "name": "fec-cms",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/18F/fec-cms.git"
  },
  "scripts": {
    "build": "cd fec && webpack && gulp build-sass && gulp build-widgets-sass",
    "build-icons": "cd fec && gulp minify-icons && gulp consolidate-icons",
    "build-sass": "cd fec && gulp build-sass && gulp build-widgets-sass",
    "build-widgets-sass": "cd fec && gulp build-widgets-sass",
    "build-js": "cd fec && webpack",
    "build-production": "cd fec && webpack -p && gulp build-sass && gulp build-widgets-sass",
    "format": "prettier --write ' ./fec/fec/static/js/**/*.js'",
    "lint": "eslint --quiet ./fec/fec/static/js/**/*.js",
    "format-lint": "npm run format && npm run lint",
    "test": "karma start",
    "test-single": "npm run lint && karma start --single-run --browsers ChromeHeadless",
    "watch": "nswatch"
  },
  "dependencies": {
    "a11y-dialog": "4.0.0",
    "accessible-mega-menu": "git+https://github.com/noahmanger/Accessible-Mega-Menu.git",
