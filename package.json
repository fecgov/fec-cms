    "karma-coverage": "^2.2.1",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-mocha": "^2.0.1",
    "karma-webpack": "^5.0.1",
    "leaflet": "^1.9.4",
    "leaflet-providers": "^2.0.0",
    "list.js": "^2.3.1",
