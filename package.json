    "babelify": "^7.3.0",
    "braces": "^2.3.2",
    "browserify": "^16.2.3",
    "browserify-istanbul": "^3.0.1",
    "cache-loader": "^1.2.5",
    "chai": "^3.4.1",
    "clean-webpack-plugin": "^0.1.16",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "eslint": "^5.14.1",
