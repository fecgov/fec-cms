    "es6-weak-map": "2.0.1",
    "eventemitter2": "0.4.14",
    "fullcalendar": "3.3.1",
    "glossary-panel": "1.0.0",
    "jquery": "^3.4.1",
    "jquery.inputmask": "3.3.4",
    "leaflet-providers": "1.1.6",
    "lodash": "^4.17.14",
    "minimatch": "^3.0.4",
    "moment": "2.20.1",
    "numeral": "^1.5.3",
    "perfect-scrollbar": "0.6.2",
    "prop-types": "^15.7.2",
    "rangy": "^1.3.0",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-input-mask": "^2.0.3",
